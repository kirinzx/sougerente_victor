{
  "expo": {
    "name": "SouGerente",
    "slug": "kirin-project",
    "description": "React Native - Soft UI Kit",
    "primaryColor": "#CB0C9F",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash_.png",
      "resizeMode": "cover",
      "backgroundColor": "#FFFFFF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "SouGerente"
    },
    "android": {
      "package": "com.kirinzx.kirinproject",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png"
      }
    }
  }
}
